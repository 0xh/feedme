{
    "name": "peehaa/feedme",
    "description": "Asynchronous feed reader",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Pieter Hordijk",
            "email": "info@pieterhordijk.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "ext-dom": "*",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "amphp/amp": "^2.1.2",
        "amphp/artax": "^3.0.14",
        "amphp/byte-stream": "^1.6.0",
        "amphp/file": "^0.3.5",
        "amphp/http": "^1.2.0",
        "amphp/http-server": "^1.1.0",
        "amphp/http-server-router": "^1.0.1",
        "amphp/http-server-session": "^1.0.0",
        "amphp/http-server-static-content": "^1.0.3",
        "amphp/log": "^1.0.0",
        "amphp/parallel": "^1.2.0",
        "amphp/parallel-functions": "^0.1.3",
        "amphp/postgres": "^1.0.4",
        "amphp/redis": "^0.3.3",
        "amphp/websocket-server": "^1.0.1",
        "harmonyio/validation": "1.0.0-rc1",
        "opis/json-schema": "^1.0.18",
        "psr/log": "^1.1.0",
        "rdlowrey/auryn": "^1.4.2",
        "robmorgan/phinx": "^0.10.7"
    },
    "require-dev": {
        "harmonyio/php-codesniffer-ruleset": "dev-master",
        "harmonyio/phpunit-extension": "dev-master",
        "maglnet/composer-require-checker": "^2.0.0",
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "^8.2.3",
        "slevomat/coding-standard": "^5.0.4",
        "squizlabs/php_codesniffer": "^3.4.2"
    },
    "autoload": {
        "psr-4": {
            "PeeHaa\\FeedMe\\": "src/"
        },
        "files": [
           "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PeeHaa\\FeedMeTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
